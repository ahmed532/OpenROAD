
Header:
  version: "1.0"
  unit: "micron"
  precision: 2000
  include:
    - upper.3dbx

Design:
  name: "DeepTopDesign"

ChipletInst:
  upper_inst:
    reference: UpperChip
  leaf_inst_top:
    reference: LeafChip

Stack:
  upper_inst:
    loc: [1000.0, 1000.0]
    z: 300.0
    orient: R0
  leaf_inst_top:
    loc: [1000.0, 1000.0]
    z: 0.0
    orient: R0

Connection:
  deep_conn:
    top: upper_inst/mid_inst/leaf_inst.regions.back_reg
    bot: leaf_inst_top.regions.front_reg
    thickness: 0.0
