Header:
  version: "1.0"
  unit: "micron"
  precision: 2000
  include:
    - example.3dbv

Design:
  name: "TopDesign"

ChipletInst:
  soc_inst:
    reference: SoC
  soc_inst_duplicate:
    reference: SoC

Stack:
  soc_inst:
    loc: [100.0, 200.0]
    z: 0.0
    orient: R0
  soc_inst_duplicate:
    loc: [100.0, 200.0]
    z: 0.0
    orient: R0

Connection:
  soc_to_soc:
    top: soc_inst_duplicate.regions.r1
    bot: soc_inst.regions.r1
    thickness: 0.0
